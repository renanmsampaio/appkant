{
    "routes": [
        {
            "route": "/*",
            "allowedRoles": ["authenticated"]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/aad"
        },
        "403": {
            "statusCode": 302,
            "redirect": "/unauthorized"
        }
    }
}
